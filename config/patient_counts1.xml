	<input name="month1" type="int" id="3"/>
	<input name="start_date1" type="date" format="yyyymmdd" id="4"/>
	<input name="end_date1" type="date" format="yyyymmdd" id="5"/>

	<input name="codes1" type="csv" id="0">
		<columns>
			<column name="code" pos="1" datatype="str" format=""/>
